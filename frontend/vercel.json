{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://chatty-online-chataplication-backend.onrender.com/api/$1",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}